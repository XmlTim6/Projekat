SELECT DISTINCT ?paper ?title ?received ?revised ?accepted FROM <%s>
WHERE {
  	?author <http://www.tim6.rs/predicate/firstName> ?firstName .
    ?author <http://www.tim6.rs/predicate/middleName> ?middleName .
    ?author <http://www.tim6.rs/predicate/lastName> ?lastName .
	?author <http://www.tim6.rs/predicate/wrote> ?paper .
  	?paper <http://www.tim6.rs/predicate/title> ?title .
  	?paper <http://www.tim6.rs/predicate/received> ?received .
  	?paper <http://www.tim6.rs/predicate/revised> ?revised .
    ?paper <http://www.tim6.rs/predicate/accepted> ?accepted .
  FILTER(CONTAINS(UCASE(CONCAT(?firstName, ?middleName, ?lastName)), UCASE("%s")))
}